<?php

namespace SmartDev\MainBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * TypeProduit
 */
class TypeProduit
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $nomTypeProduit;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set nomTypeProduit
     *
     * @param string $nomTypeProduit
     * @return TypeProduit
     */
    public function setNomTypeProduit($nomTypeProduit)
    {
        $this->nomTypeProduit = $nomTypeProduit;
    
        return $this;
    }

    /**
     * Get nomTypeProduit
     *
     * @return string 
     */
    public function getNomTypeProduit()
    {
        return $this->nomTypeProduit;
    }
}