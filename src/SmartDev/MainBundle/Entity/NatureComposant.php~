<?php

namespace SmartDev\MainBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * NatureComposant
 */
class NatureComposant
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $nomNaure;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set nomNaure
     *
     * @param string $nomNaure
     * @return NatureComposant
     */
    public function setNomNaure($nomNaure)
    {
        $this->nomNaure = $nomNaure;
    
        return $this;
    }

    /**
     * Get nomNaure
     *
     * @return string 
     */
    public function getNomNaure()
    {
        return $this->nomNaure;
    }
}