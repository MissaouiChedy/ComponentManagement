{%extends '::base.html.twig'%}

{% block title%}
    Composants
{% endblock %}

{%block body%}
    <table id="components" style="border: 2px solid black">
        <tr>
           {% for header in model.headers %}
               <th style="border: 2px solid black">{{header}}</th>
           {% endfor %}
        </tr>
        {% for row in model.rows %}
            <tr>
               {% for column in row %}
                 <td style="border: 2px solid black"> {{ column }}</td>
               {% endfor %}
            </tr>
        {% endfor %}
    </table>
{%endblock%}
